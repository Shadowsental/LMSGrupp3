@model LMSGrupp3.Models.Entities.Course

@{
    ViewData["Title"] = "Course Student List";
}

<h1>Courses</h1>

<th>
    @Html.DisplayNameFor(model => model.Name)
</th>

<table class="table">
    <tbody>
@foreach (var item in Model.Users.Where(a => a.User.Email.Contains("student"))) {
        <tr>
            <div class="row studentRow">
                <div class="col-sm-10 font-weight-bold">
                    @item.User.Name
                </div>
            </div>
        </tr>
}
    </tbody>
</table>